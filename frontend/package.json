{
  "name": "carrito-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "node node_modules/@angular/cli/bin/ng.js",
    "start": "node node_modules/@angular/cli/bin/ng.js serve",
    "serve": "node node_modules/@angular/cli/bin/ng.js serve --port 4200",
    "dev": "node node_modules/@angular/cli/bin/ng.js serve --port 4200 --open",
    "build": "node node_modules/@angular/cli/bin/ng.js build",
    "watch": "node node_modules/@angular/cli/bin/ng.js build --watch --configuration development",
    "test": "node node_modules/@angular/cli/bin/ng.js test",
    "start:dev": "node node_modules/@angular/cli/bin/ng.js serve --configuration development --port 4200",
    "start:test": "node node_modules/@angular/cli/bin/ng.js serve --configuration test --port 4201",
    "start:staging": "node node_modules/@angular/cli/bin/ng.js serve --configuration staging --port 4202", 
    "start:prod": "node node_modules/@angular/cli/bin/ng.js serve --configuration production --port 4203",
    "build:dev": "node node_modules/@angular/cli/bin/ng.js build --configuration development",
    "build:test": "node node_modules/@angular/cli/bin/ng.js build --configuration test",
    "build:staging": "node node_modules/@angular/cli/bin/ng.js build --configuration staging",
    "build:prod": "node node_modules/@angular/cli/bin/ng.js build --configuration production",
    "analyze": "node node_modules/@angular/cli/bin/ng.js build --configuration production --stats-json && npx webpack-bundle-analyzer dist/carrito-app/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.0"
  }
}